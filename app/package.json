{
    "private": true,
    "scripts": {
        "test-vue": "cross-env NODE_ICU_DATA=node_modules/full-icu node_modules/.bin/jest --verbose",
        "es-lint": "node_modules/.bin/eslint resources --ext .js,.vue",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@types/jest": "^25.2.1",
        "@vue/test-utils": "^1.0.0-beta.33",
        "axios": "^0.19.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^25.3.0",
        "babel-loader": "^8.1.0",
        "bootstrap": "^4.4.1",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-eslint": "^6.0.0",
        "eslint-plugin-vue": "^6.2.2",
        "full-icu": "^1.3.1",
        "jest": "^25.3.0",
        "jquery": "^3.5.0",
        "laravel-mix": "^5.0.4",
        "lodash": "^4.17.5",
        "npm": "^6.14.4",
        "popper.js": "^1.16.1",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.11",
        "vue-jest": "^3.0.4",
        "vue-router": "^3.1.6",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@babel/runtime": "^7.9.2",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "moment": "^2.24.0",
        "v-calendar": "^1.0.3",
        "vue-js-toggle-button": "^1.3.3",
        "vue-top-progress": "^0.7.0",
        "vuelidate": "^0.7.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            ".*\\.(vue)$": "vue-jest"
        },
        "collectCoverageFrom": [
            "**/js/components/tools/*.vue",
            "**/js/components/Finance/**/*.vue",
            "!**/node_modules/**",
            "!**/vendor/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 70,
                "lines": 90,
                "statements": 90
            }
        },
        "coverageReporters": [
            "text"
        ]
    }
}
